{
  "name": "live_flow",
  "version": "0.1.0",
  "description": "Interactive node-based flow diagrams for Phoenix LiveView",
  "license": "MIT",
  "type": "module",
  "module": "./priv/static/live_flow.esm.js",
  "exports": {
